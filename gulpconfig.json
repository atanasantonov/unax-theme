{
  "browserSyncOptions" : {
    "proxy": "http://wordpress.local/",
    "notify": true
  },
  "browserSyncWatchFiles" : [
    "./*.min.css",
    "./dist/js/*.min.js",
    "./sass/**/*.scss",
    "./**/*.php"
  ],
  "paths" : {
    "js": "./js",
    "css": "./",
    "fonts": "./fonts",
    "img": "./img",
	"imgsrc": "./src/img",
    "sass": "./sass",
    "src": "./src",
    "dist": "./dist",
    "node": "./node_modules/",
    "vendor": ""
  }
}
